{
  "description": "transfers timestamp_precise to @timestamp if present",
  "processors": [
    {
      "remove": {
        "field": "@timestamp",
        "if": "ctx.containsKey('timestamp_precise')"
      }
    },
    {
      "rename": {
        "field": "timestamp_precise",
        "target_field": "@timestamp",
        "ignore_missing": true
      }
    }
  ]
}
